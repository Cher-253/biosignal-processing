	$ ->
		window.pm = new ProfileManager
			onLoad: ()->
				handlers ()->
					window.ui = new UIManager()
					window.plt = new PlotManager()
					$(".paper-plot").trigger('load')
				$('#'+111).click() # DEFAULT USER

	

		